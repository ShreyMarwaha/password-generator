{
    "name": "@nikitababko/password-generator",
    "version": "0.0.1-alfa.16",
    "description": "Password generator",
    "main": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "scripts": {
        "start:dev": "ts-node src/index.ts",
        "start:example": "ts-node tests/index.ts",
        "clear": "rm -rf dist",
        "build": "tsc",
        "build:lib": "npm run clear && tsc && tsc -m es6 --outDir dist && webpack",
        "test:all": "mocha './src/index.spec.ts'",
        "pre:commit": "yarn pre:release",
        "pre:release": "npm --no-git-tag-version version prerelease",
        "pre:patch": "npm --no-git-tag-version version patch",
        "pre:minor": "npm --no-git-tag-version version minor",
        "pre:major": "npm --no-git-tag-version version major"
    },
    "keywords": [
        "password",
        "generator",
        "password-generator",
        "crypt",
        "encryption"
    ],
    "author": "Nikita Babko <niki.babko@gmail.com>",
    "license": "ISC",
    "private": false,
    "sideEffects": false,
    "engines": {
        "node": ">=14.17.5"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/nikitababko/password-generator.git"
    },
    "bugs": {
        "url": "https://github.com/nikitababko/password-generator/issues"
    },
    "homepage": "https://github.com/nikitababko/password-generator#readme",
    "devDependencies": {
        "husky": "8.0.1",
        "@types/chai": "4.3.1",
        "@types/mocha": "9.1.1",
        "@types/node": "17.0.42",
        "chai": "4.3.6",
        "mocha": "10.0.0",
        "ts-loader": "9.3.0",
        "ts-node": "10.8.1",
        "typescript": "4.7.3",
        "webpack": "5.73.0",
        "webpack-cli": "4.9.2"
    }
}
